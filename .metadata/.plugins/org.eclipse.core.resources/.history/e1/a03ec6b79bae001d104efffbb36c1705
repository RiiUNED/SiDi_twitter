package Comun;

import java.rmi.*;

/*
 * Interfaz remota del servidor
 * @autor: rsanchez628@alumno.uned.es
 * 			Ricardo Sanchez	
 */

public interface ServerInt extends Remote{
	
	/*
	 * Registra usuarios en la aplicacion
	 * @autor: rsanchez628@alumno.uned.es
	 * 			Ricardo Sanchez
	 */
	public void autenticarse (Usuario user, HashMap<String, Usuario> BBDD) throws java.rmi.RemoteException;

}
